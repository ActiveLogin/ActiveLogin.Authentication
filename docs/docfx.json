{
    "metadata": [{
        "src": [{
            "src": "../",
            "files": [
                "src/**.csproj"
            ],
            "exclude": [
                "**/bin/**",
                "**/obj/**"
            ]
        }],
        "dest": "api",
        "disableGitFeatures": false,
        "disableDefaultFilter": false
    }],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [ "../README.MD" ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [{
            "files": [
                "images/**"
            ]
        }],
        "overwrite": [{
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "_site/**"
            ]
        }],
        "dest": "_site",
        "globalMetadata": {
            "_enableSearch": true,
            "_enableNewTab": true,
            "_appFaviconPath": "/images/favicon.ico",
            "_appTitle": "Active Login",
            "_appFooter": "Â© Active Solution",
            "_disableContribution": false,
            "_gitContribute": {
                "repo": "https://github.com/ActiveLogin/ActiveLogin.Authentication",
                "branch": "main",
                "apiSpecFolder": "docs/apiSpec"
            }
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/custom_template"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "properties": {
            "TargetFramework": "net6.0"
        }
    }
}
