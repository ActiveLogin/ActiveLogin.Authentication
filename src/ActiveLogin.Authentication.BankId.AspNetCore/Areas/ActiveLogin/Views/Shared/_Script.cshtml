@using ActiveLogin.Authentication.BankId.AspNetCore.Areas.ActiveLogin.Models
@model BankIdUiViewModel

<script src="~/_content/ActiveLogin.Authentication.BankId.AspNetCore/activelogin-main.js" asp-append-version="true"></script>
<script>
    (function (configuration, initState, signData) {
        window.activeloginInit(configuration, initState, signData);
    }(
        @Html.Raw(Model.BankUiScriptConfigurationJson()),
        @Html.Raw(Model.BankIdUiScriptInitStateJson()),
        @Html.Raw(Model.BankIdUiSignDataAsJson()))
    );
</script>
